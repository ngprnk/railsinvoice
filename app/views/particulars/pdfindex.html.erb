<%= wisepdf_stylesheet_tag "bootstrap.min" %>



<div class="container" style="margin-top: 100px;">
  <p id="notice" ><%= notice %></p>

  <div class="container col-md-8 col-md-offset-1" style="border: 1px solid #e3e3e3">


    <h1 style="margin-left: 20px;">Items</h1>

    <div class="col-xs-11 col-sm-11 col-md-11 col-lg-11">
      <table class="table table-hover">
        <thead>
          <tr class="bg-primary">
            <th>Item Name</th>
            <th>Quantity </th>
            <th>Rate</th>
            <th>Amount</th>
            

          </tr>
        </thead>
        <tbody>
         <tbody>
          <% @particulare.each do |particular| %>
          <tr>
            <td><%= particular.itemname %></td>
            <td><%= particular.quantity %></td>
            <td>RM <%= particular.rate %></td>
            <td>RM <%= particular.quantity * particular.rate %></td>


            
            
          </tr>
          <% end %>
        </tbody>
      </table>
     
    </div>
  </div>




